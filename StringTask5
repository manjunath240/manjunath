package StringTask;

public class StringTask5 {

	public static void main(String[] args) {
		String givenString="Fisglobal bangalore karnataka";
		char[] ch=givenString.trim().toCharArray();
		int count=0;
		for(int i=0;i<givenString.length()-1;i++)
		{
			if(ch[i]==' ')
			{
				count=count+1;
			}
		}
		count=count+1;
System.out.println("The number of words are"+count);
	}

}
